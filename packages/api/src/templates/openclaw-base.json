{
  "meta": {
    "lastTouchedVersion": "2026.2.x",
    "winston": {
      "tenantId": "{{TENANT_ID}}",
      "tier": "{{TIER}}",
      "baselineVersion": "1.0"
    }
  },
  "gateway": {
    "port": 18789,
    "mode": "local",
    "bind": "loopback",
    "auth": {
      "mode": "token",
      "token": "{{GATEWAY_TOKEN}}"
    }
  },
  "models": {
    "providers": {
      "winston": {
        "baseUrl": "{{LLM_PROXY_URL}}",
        "apiKey": "winston-{{TENANT_ID}}",
        "api": "openai-completions",
        "models": [
          {
            "id": "kimi-k2.5",
            "name": "Kimi K2.5 (Default - Most Affordable)",
            "reasoning": true,
            "input": ["text", "image"],
            "contextWindow": 262000,
            "maxTokens": 8192
          },
          {
            "id": "claude-sonnet-4-5",
            "name": "Claude Sonnet 4.5 (5x credits)",
            "reasoning": true,
            "input": ["text", "image"],
            "contextWindow": 200000,
            "maxTokens": 8192
          },
          {
            "id": "claude-opus-4-6",
            "name": "Claude Opus 4.6 (12x credits)",
            "reasoning": true,
            "input": ["text", "image", "video"],
            "contextWindow": 200000,
            "maxTokens": 16384
          }
        ]
      }
    }
  },
  "agents": {
    "defaults": {
      "model": "winston/{{SELECTED_MODEL}}",
      "workspace": "/data/workspace",
      "memorySearch": {
        "enabled": true,
        "experimental": {
          "sessionMemory": true
        },
        "sources": ["memory", "sessions"]
      },
      "tools": {
        "profile": "minimal",
        "allow": ["read", "web_search", "memory_search", "memory_get"],
        "deny": ["exec", "write", "edit", "browser", "cron", "nodes"]
      },
      "messages": {
        "contextOverflow": {
          "strategy": "auto-compact",
          "softThresholdTokens": 60000
        }
      }
    }
  },
  "channels": {
    "telegram": {
      "enabled": {{TELEGRAM_ENABLED}},
      "botToken": "{{TELEGRAM_BOT_TOKEN}}"
    },
    "slack": {
      "enabled": {{SLACK_ENABLED}}
    },
    "whatsapp": {
      "enabled": {{WHATSAPP_ENABLED}}
    }
  },
  "skills": {
    "entries": {}
  }
}
